{
  "name": "zkp-multiplier3",
  "version": "1.0.0",
  "description": "ZKP project with 3-number multiplier circuit",
  "main": "index.js",
  "scripts": {
    "compile": "circom circuits/Multiplier3.circom --r1cs --wasm --sym -o circuits",
    "setup": "snarkjs groth16 setup circuits/Multiplier3.r1cs pot12_final.ptau circuits/Multiplier3.zkey && snarkjs zkey export verificationkey circuits/Multiplier3.zkey circuits/verification_key.json",
    "generate-proof": "node scripts/generate-proof.js",
    "verify": "node scripts/verify.js",
    "generate-verifier": "snarkjs zkey export solidityverifier circuits/Multiplier3.zkey contracts/Verifier.sol",
    "deploy": "npx hardhat run scripts/deploy.js --network localhost"
  },
  "dependencies": {
    "circom": "^0.5.46",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "dotenv": "^16.5.0",
    "fixed-merkle-tree": "^0.7.3",
    "snarkjs": "^0.7.5"
  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync-deploy": "^1.7.0",
    "@matterlabs/hardhat-zksync-solc": "^1.3.0",
    "@matterlabs/hardhat-zksync-verify": "^1.8.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^4.9.0",
    "glob": "^10.3.10",
    "hardhat": "^2.23.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
